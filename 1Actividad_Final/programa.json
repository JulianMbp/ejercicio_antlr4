{
  "source": {
    "file": "empleados.csv"
  },
  "filters": [
    {
      "column": "edad",
      "operator": ">",
      "value": 25
    },
    {
      "column": "salario", 
      "operator": ">",
      "value": 30000
    }
  ],
  "aggregations": [
    {
      "type": "count",
      "column": "id"
    },
    {
      "type": "average",
      "column": "salario"
    },
    {
      "type": "sum",
      "column": "dias_laborados"
    }
  ],
  "output": {
    "print": true
  }
} 