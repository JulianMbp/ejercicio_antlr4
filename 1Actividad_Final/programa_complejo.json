{
  "source": {
    "file": "empleados.csv"
  },
  "filters": [
    {
      "type": "AND",
      "conditions": [
        {
          "column": "edad",
          "operator": ">",
          "value": 30
        },
        {
          "column": "salario",
          "operator": "<",
          "value": 5000000
        }
      ]
    },
    {
      "type": "OR",
      "conditions": [
        {
          "column": "dias_laborados",
          "operator": ">",
          "value": 2000
        },
        {
          "column": "nombre",
          "operator": "contains",
          "value": "Garc√≠a"
        }
      ]
    }
  ],
  "aggregations": [
    {
      "type": "count",
      "column": "id"
    },
    {
      "type": "average",
      "column": "salario"
    },
    {
      "type": "max",
      "column": "edad"
    },
    {
      "type": "min",
      "column": "edad"
    }
  ],
  "output": {
    "print": true,
    "max_rows": 10
  }
} 