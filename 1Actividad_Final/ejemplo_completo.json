{
  "source": {
    "file": "empleados.csv"
  },
  "filters": [
    {
      "type": "AND",
      "conditions": [
        {
          "column": "edad",
          "operator": "in_range",
          "value": [35, 55]
        },
        {
          "type": "OR",
          "conditions": [
            {
              "column": "salario",
              "operator": "greater",
              "value": 4500000
            },
            {
              "column": "dias_laborados",
              "operator": "greater",
              "value": 2000
            }
          ]
        }
      ]
    },
    {
      "type": "OR",
      "conditions": [
        {
          "column": "nombre",
          "operator": "contains",
          "value": "García"
        },
        {
          "column": "nombre",
          "operator": "contains",
          "value": "Hernández"
        },
        {
          "column": "nombre",
          "operator": "ends_with",
          "value": "ez"
        }
      ]
    },
    {
      "column": "id",
      "operator": "mod",
      "value": 2
    },
    {
      "column": "calc:int({edad}) * int({dias_laborados})",
      "operator": "greater",
      "value": 50000
    }
  ],
  "aggregations": [
    {
      "type": "count",
      "column": "id"
    },
    {
      "type": "average",
      "column": "salario"
    },
    {
      "type": "median",
      "column": "edad"
    },
    {
      "type": "std_dev",
      "column": "salario"
    },
    {
      "type": "max",
      "column": "dias_laborados"
    },
    {
      "type": "min",
      "column": "edad"
    }
  ],
  "output": {
    "print": true,
    "max_rows": 10
  }
} 